complete -F _command $filenames screen
complete -F _command $filenames watch
complete -F _command $filenames tsplit
complete -F _command $filenames twin
complete -F _known_hosts nmap
complete -F _known_hosts rdesktop

_wpa_cli()
{
	local cur commands
	commands="status mib help interface level license logoff logon set pmksa reassociate reconfigure preauthenticate identity password new_password pin otp passphrase bssid list_networks select_network enable_network disable_network add_network remove_network set_network get_network save_config disconnect reconnect scan scan_results bss get_capability ap_scan stkstart ft_ds terminate quit"
	COMPREPLY=()
	cur=${COMP_WORDS[COMP_CWORD]}
	COMPREPLY=( $(compgen -W "$commands" -- "$cur") )
} &&
complete -F _wpa_cli wpa_cli
