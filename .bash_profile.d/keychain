# -*- mode: sh; -*-
command -v keychain >/dev/null 2>&1 || return

# Do not start keychain if gpg is configured with ssh-support
if [ "$(gpgconf --list-options gpg-agent | grep '^enable-ssh-support:' | cut -d : -f 10)" = "1" ]; then
  return
fi

eval $(keychain --eval --agents ssh --inherit any-once --quiet)
